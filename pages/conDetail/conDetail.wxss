scroll-view {
  height: 88vh;
}
scroll-view .con_detail .con_data {
  height: 40vh;
  background-color: #3e535a;
}
scroll-view .con_detail .con_data > view {
  margin: 0 auto;
  width: 95%;
}
scroll-view .con_detail .con_data .con_data_top {
  height: 20vh;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
scroll-view .con_detail .con_data .con_data_top > view {
  width: 100%;
  height: 80%;
  display: flex;
}
scroll-view .con_detail .con_data .con_data_top > view .con_image {
  flex: 1;
  height: 180rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_image image {
  height: 80%;
  width: 80%;
  border-radius: 50%;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user {
  flex: 3;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user > view {
  display: flex;
  align-items: center;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_name {
  align-items: flex-start;
  height: 60rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_name > text {
  font-weight: bold;
  color: white;
  font-size: 40rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_title {
  height: 60rpx;
  font-size: 25rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_title > text {
  color: #ff9a83;
  padding: 0 15rpx 0;
  background-color: white;
  border-radius: 20rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_registDate {
  height: 70rpx;
  font-size: 28rpx;
}
scroll-view .con_detail .con_data .con_data_top > view .con_user .con_registDate > text {
  color: #ff9a83;
  padding: 1rpx 15rpx 1rpx;
  background-color: #616db5;
  border-radius: 20rpx;
}
scroll-view .con_detail .con_data .con_data_bottom .con_lable {
  display: flex;
  align-items: center;
  height: 70rpx;
  color: rgba(255, 255, 255, 0.679);
  font-size: 28rpx;
}
scroll-view .con_detail .con_data .con_data_bottom .achieve {
  display: flex;
  font-size: 30rpx;
  color: rgba(255, 255, 255, 0.734);
}
scroll-view .con_detail .con_data .con_data_bottom .achieve > view {
  flex: 1;
}
scroll-view .con_detail .con_data .con_data_bottom .achieve > view > view {
  display: flex;
  align-items: center;
  justify-content: center;
}
scroll-view .con_detail .con_data .con_data_bottom .achieve > view > view:first-child {
  height: 80rpx;
}
scroll-view .con_detail .con_data .con_data_bottom .achieve > view > view:last-child {
  border-right: 1rpx solid black;
}
scroll-view .con_detail .con_data .con_data_bottom .achieve .con_likes > view:last-child {
  border-style: none;
}
scroll-view .con_detail .con_intro {
  height: 60vh;
  font-size: 35rpx;
  text-indent: 20px;
}
scroll-view .con_detail .con_intro > view:first-child {
  font-size: 45rpx;
  text-indent: 20px;
}
scroll-view .con_detail .con_intro > view:last-child > view:last-child {
  height: 100rpx;
}
.footer_btn {
  height: 180rpx;
  width: 100%;
  border-radius: 50rpx 50rpx 0 0;
  position: absolute;
  bottom: 0;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
}
.footer_btn > view {
  height: 100rpx;
  width: 85%;
  display: flex;
  justify-content: space-between;
}
.footer_btn > view .toMessage {
  font-size: 28rpx;
  color: #828e9d;
}
.footer_btn > view .toMessage > icon {
  font-size: 45rpx;
}
.footer_btn > view .toOrder {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
  width: 450rpx;
  border-radius: 50rpx;
  background-color: #8596f9;
  box-shadow: inset 2px 2px 2px 0px rgba(255, 255, 255, 0.3), 7px 7px 20px 0px rgba(0, 0, 0, 0.1), 4px 4px 5px 0px rgba(0, 0, 0, 0.1);
  color: white;
}
.add_card {
  display: none;
  height: 100vh;
  position: absolute;
  right: 0;
  left: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.3);
}
.add_card .footer_addEnquiry {
  width: 100%;
  border-radius: 50rpx 50rpx 0 0;
  position: absolute;
  bottom: -1000rpx;
  background-color: #f1f1f1;
  display: flex;
  flex-direction: column;
  transition: bottom 4s;
}
.add_card .footer_addEnquiry .footer_title {
  font-size: 40rpx;
  font-weight: bold;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  height: 100rpx;
}
.add_card .footer_addEnquiry .footer_label {
  height: 100rpx;
  display: flex;
  align-items: flex-end;
  font-size: 30rpx;
  color: #6a6a6a;
}
.add_card .footer_addEnquiry .footer_scroll {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view {
  height: 620rpx;
  width: 95%;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view .dateItem {
  height: 120rpx;
  padding: 10rpx;
  display: flex;
  align-items: center;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view .dateItem .date {
  width: 100rpx;
  height: 100rpx;
  font-size: 30rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view .dateItem .status {
  height: 100rpx;
  margin-left: 20rpx;
  display: flex;
  align-items: center;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view .dateItem .status > view {
  height: 100%;
  width: 100rpx;
  border-radius: 20rpx;
  margin-right: 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
  font-size: 25rpx;
  background-color: white;
  color: black;
  border: 1rpx solid #d6d6d6;
}
.add_card .footer_addEnquiry .footer_scroll > scroll-view .dateItem .status > view > view:nth-child(2) {
  width: 10rpx;
  margin: 8rpx;
  height: 1rpx;
  background-color: #898989;
}
.add_card .footer_addEnquiry .addBtn {
  height: 120rpx;
  display: flex;
  align-items: center;
}
.add_card .footer_addEnquiry .addBtn > button {
  height: 90rpx;
  width: 90%;
  padding: 0;
  border-radius: 50rpx;
  background-color: #8596f9;
  box-shadow: inset 2px 2px 2px 0px rgba(255, 255, 255, 0.3), 7px 7px 20px 0px rgba(0, 0, 0, 0.1), 4px 4px 5px 0px rgba(0, 0, 0, 0.1);
  color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  font-weight: normal;
  font-size: 35rpx;
}
.weekHander {
  color: blue;
}
.dateHander {
  color: white;
  background-color: lightblue;
}
