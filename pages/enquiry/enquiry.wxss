scroll-view {
  height: 86.3vh;
}
scroll-view .en_content {
  height: 100%;
  width: 95%;
  margin: 0 auto;
}
scroll-view .en_content .en_head {
  height: 50rpx;
}
scroll-view .en_content .en_item {
  width: 100%;
  display: flex;
  border-bottom: 1rpx solid rgba(0, 0, 0, 0.141);
  margin-top: 50rpx;
}
scroll-view .en_content .en_item .en_item_left {
  flex: 1;
  height: 110rpx;
  display: flex;
  align-items: center;
}
scroll-view .en_content .en_item .en_item_left image {
  width: 85%;
  height: 85%;
  border-radius: 50%;
}
scroll-view .en_content .en_item .en_item_right {
  flex: 5;
}
scroll-view .en_content .en_item .en_item_right .item_header {
  display: flex;
}
scroll-view .en_content .en_item .en_item_right .item_header .item_name {
  flex: 1;
  font-size: 35rpx;
  font-weight: bold;
  display: flex;
  align-items: center;
}
scroll-view .en_content .en_item .en_item_right .item_header .item_detail {
  flex: 3;
  font-size: 25rpx;
  display: flex;
  align-items: flex-end;
  font-weight: 600;
  color: rgba(0, 0, 0, 0.504);
}
scroll-view .en_content .en_item .en_item_right .item_title .item_intro {
  font-size: 30rpx;
  color: #6d798b;
  margin-top: 10rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
scroll-view .en_content .en_item .en_item_right .item_title > view:nth-child(2) {
  height: 80rpx;
  width: 100%;
  font-size: 30rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  color: #6d798b;
}
scroll-view .en_content .en_item .en_item_right .item_title > view:nth-child(2) .item_label {
  background-color: #f2f4ff;
  width: 380rpx;
  height: 45rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
scroll-view .en_content .en_item .en_item_right .item_title > view:nth-child(2) .item_address {
  width: 200rpx;
  height: 40rpx;
  display: flex;
  align-items: center;
  color: #bcc2d0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
scroll-view .en_content .en_item .en_item_right .item_title > view:nth-child(2) .item_address > icon {
  display: flex;
  align-items: flex-end;
  height: 35rpx;
  font-size: 20rpx;
}
scroll-view .en_content .en_item .en_item_right .item_title .item_likes {
  display: flex;
  align-items: center;
  height: 60rpx;
  font-size: 25rpx;
}
scroll-view .en_content .en_item .en_item_right .item_title .item_likes > text {
  padding: 3rpx;
  border: 5rpx solid lightgreen;
  border-radius: 10rpx;
  color: lightgreen;
  margin-right: 20rpx;
  font-size: 30rpx;
}
scroll-view .en_content .en_item .en_item_right .item_title .item_time {
  height: 70rpx;
  font-size: 30rpx;
  display: flex;
  align-items: center;
  color: #8297fd;
}
scroll-view .en_content .en_item .en_item_right .item_title .item_time > icon {
  display: flex;
  align-items: flex-end;
  height: 35rpx;
  font-size: 30rpx;
  margin-right: 10rpx;
}
scroll-view .en_content .en_item .en_item_right .item_comment {
  height: 200rpx;
  border-radius: 20rpx;
  background-color: #f3f4f6;
  display: flex;
  flex-direction: column;
}
scroll-view .en_content .en_item .en_item_right .item_comment .comment_header {
  flex: 2;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
scroll-view .en_content .en_item .en_item_right .item_comment .comment_header .comment_hearder_left {
  flex: 1.3;
  text-indent: 30rpx;
  font-size: 30rpx;
}
scroll-view .en_content .en_item .en_item_right .item_comment .comment_header .comment_hearder_right {
  flex: 1;
  height: 50%;
  font-size: 25rpx;
  display: flex;
  align-items: flex-end;
  justify-content: flex-start;
  color: #9ca3ad;
}
scroll-view .en_content .en_item .en_item_right .item_comment .comment_content {
  flex: 3;
  display: flex;
  align-items: center;
  justify-content: center;
}
scroll-view .en_content .en_item .en_item_right .item_comment .comment_content > text {
  width: 90%;
  font-size: 30rpx;
  text-indent: 45rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  color: #70778a;
}
scroll-view .en_content .en_item:nth-child(2) {
  margin-top: 0rpx;
}
scroll-view .en_content .en_end {
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: rgba(0, 0, 0, 0.4);
}
