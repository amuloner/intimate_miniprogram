scroll-view {
  height: 100vh;
  display: flex;
  justify-content: center;
}
scroll-view .cvs_page {
  background-color: rgba(186, 186, 186, 0.252);
}
scroll-view .cvs_page .cvs_header {
  display: flex;
  background-color: white;
  margin-bottom: 20rpx;
}
scroll-view .cvs_page .cvs_header .userImg {
  width: 120rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
scroll-view .cvs_page .cvs_header .userImg image {
  height: 70%;
  width: 70%;
  border-radius: 50%;
}
scroll-view .cvs_page .cvs_header .cvs_container {
  width: 82%;
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_title {
  height: 150rpx;
  display: flex;
  align-items: center;
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_title > view {
  font-size: 40rpx;
  font-weight: bold;
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_detail {
  height: 50rpx;
  display: flex;
  font-size: 28rpx;
  color: rgba(141, 141, 141, 0.753);
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_detail .cvs_date {
  margin-right: 40rpx;
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_content > view:first-child {
  font-size: 32rpx;
  color: #474747;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 3;
  -webkit-box-orient: vertical;
  line-height: 18px;
  margin-bottom: 20rpx;
}
scroll-view .cvs_page .cvs_header .cvs_container .cvs_content .textBtn {
  height: 60rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: rgba(0, 0, 255, 0.475);
  font-weight: bold;
}
scroll-view .cvs_page .cvs_btn {
  height: 100rpx;
  display: flex;
  font-size: 32rpx;
  justify-content: space-around;
  align-items: center;
  background-color: white;
}
scroll-view .cvs_page .cvs_btn > view {
  display: flex;
}
scroll-view .cvs_page .cvs_btn > view > view:nth-child(1) > icon {
  font-size: 50rpx;
}
scroll-view .cvs_page .cvs_btn > view > view:nth-child(2) {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column;
}
scroll-view .cvs_page .cvs_btn > view > view:nth-child(2) > view:last-child {
  font-size: 25rpx;
  color: rgba(156, 156, 156, 0.767);
}
scroll-view .cvs_page .cvs_btn .btn_toSame > view:nth-child(1) > icon {
  font-size: 45rpx;
}
scroll-view .cvs_page .cvs_replyList .noreply {
  margin-top: 20rpx;
  background-color: white;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 40rpx;
  height: 200rpx;
  color: #c5c2c2;
}
scroll-view .cvs_page .cvs_replyList .reply_item {
  margin-top: 20rpx;
  display: flex;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_left {
  width: 120rpx;
  height: 120rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_left image {
  height: 65%;
  width: 65%;
  border-radius: 50%;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right {
  width: 80%;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_userinfo {
  height: 100rpx;
  display: flex;
  align-items: center;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_userinfo .user_name {
  font-size: 35rpx;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_userinfo .item_date {
  margin-left: 20rpx;
  display: flex;
  font-size: 25rpx;
  color: rgba(141, 141, 141, 0.753);
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_content {
  margin-top: 20rpx;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_content > view:first-child {
  font-size: 32rpx;
  color: #474747;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 10;
  line-height: 18px;
  -webkit-box-orient: vertical;
  margin-bottom: 20rpx;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_content .textBtn {
  height: 60rpx;
  display: flex;
  align-items: center;
  font-size: 30rpx;
  color: rgba(0, 0, 255, 0.475);
  font-weight: bold;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_toReport {
  height: 60rpx;
  display: flex;
  align-items: flex-start;
  justify-content: flex-end;
}
scroll-view .cvs_page .cvs_replyList .reply_item .item_right .item_toReport > view {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100rpx;
  font-size: 30rpx;
  border-radius: 50rpx;
  background-color: rgba(128, 128, 128, 0.359);
}
.cvs_btn_Fixed {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  margin: 0;
  padding: 0;
  z-index: 9;
}
